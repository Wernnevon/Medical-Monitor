{
  "name": "medical-monitor",
  "productName": "Monitor Medico",
  "version": "1.0 beta",
  "description": "Electron Typescript React Boilerplate / Starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/saostad/Electron-React-Typescript.git"
  },
  "bin": "./lib/setup.js",
  "main": ".webpack/main",
  "scripts": {
    "tag": "node ./config/tag.js",
    "auto-changelog": "auto-changelog --hide-credit --breaking-pattern \"breaking:\"",
    "gen-changelog": "npm run auto-changelog && git add CHANGELOG.md && git commit -m \"doc: change log\"",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publishOnly": "electron-forge publish",
    "postpublish": "npm-run-all -s tag gen-changelog"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "starter",
    "boilerplate"
  ],
  "author": "Saeid Ostad",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Monitor Medico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/yup": "^0.29.14",
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "electron-log": "^4.4.7",
    "electron-squirrel-startup": "^1.0.0",
    "html2canvas": "^1.1.5",
    "jspdf": "^2.3.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.3",
    "unzipper": "^0.10.11",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "auto-changelog": "^2.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.1",
    "css-loader": "^5.1.1",
    "electron": "^20.1.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "node-loader": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1"
  }
}
